<idea-plugin>
    <id>com.xtu.plugins.github.trending</id>
    <name>Github-Trending</name>
    <vendor email="yanglang116@gmail.com">yanglang116@gmail.com</vendor>
    <idea-version since-build="211"/>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow id="Github Trending"
                    icon="AllIcons.Vcs.Vendors.Github"
                    anchor="right"
                    secondary="false"
                    factoryClass="com.xtu.plugin.github.TrendingWindowFactory"/>
    </extensions>
</idea-plugin>
